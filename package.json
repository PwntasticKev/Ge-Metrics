{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev:full",
    "build": "cd server && npx drizzle-kit migrate && cd .. && npm run validate:icons && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "validate:icons": "node scripts/validate-icons-simple.js",
    "test:app-loading": "node test-app-loading.cjs",
    "test:auth-bypass": "node test-auth-bypass.cjs",
    "test:simple": "node test-simple.cjs",
    "test:controlled-inputs": "node test-controlled-inputs.cjs",
    "fix:controlled-inputs": "node fix-controlled-inputs.cjs",
    "test:queryclient": "node test-queryclient.cjs",
    "test:data-loading": "node test-data-loading.cjs",
    "test:comprehensive": "node test-comprehensive.cjs",
    "test:final": "node test-final-verification.cjs",
    "collect-history": "node scripts/collectHistoryData.js",
    "collect-all-history": "node scripts/collectAllHistoricalData.js",
    "monitor-alerts": "node scripts/monitorVolumeAlerts.js",
    "db:migrate": "cd server && npm run db:migrate",
    "db:reset": "cd server && npm run db:reset",
    "db:create": "cd server && npm run db:create",
    "db:deploy": "npx prisma migrate deploy",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "6.0.22",
    "@mantine/dates": "6.0.22",
    "@mantine/form": "6.0.22",
    "@mantine/hooks": "6.0.22",
    "@mantine/notifications": "6.0.22",
    "@mantine/utils": "6.0.22",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^5.0.0",
    "@ravnhq/eslint-config": "^0.0.33",
    "@tabler/icons-react": "^2.23.0",
    "@trpc/react-query": "^11.4.2",
    "@vercel/postgres": "^0.4.1",
    "apexcharts": "^4.0.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^2.2.1",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "firebase": "^9.23.0",
    "firebaseui": "^6.0.2",
    "moment": "^2.29.4",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "recharts": "^2.15.3",
    "sass": "^1.63.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^26.1.0",
    "prisma": "^5.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
