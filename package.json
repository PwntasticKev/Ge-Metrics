{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "lint": "biome check --config-path=config/biome.json .",
    "lint:fix": "biome check --config-path=config/biome.json --write .",
    "format": "biome format --config-path=config/biome.json --write .",
    "preview": "vite preview",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vite build",
    "build:server": "cd server && npm install && npm run build",
    "start:server": "cd server && npm start",
    "start:client": "vite",
    "test": "node node_modules/vitest/dist/cli.js",
    "test:run": "node node_modules/vitest/dist/cli.js run",
    "test:unit": "node node_modules/vitest/dist/cli.js",
    "test:unit:run": "node node_modules/vitest/dist/cli.js run",
    "test:integration": "node node_modules/vitest/dist/cli.js run --config vitest.config.integration.ts",
    "test:coverage": "node node_modules/vitest/dist/cli.js run --coverage",
    "test:watch": "node node_modules/vitest/dist/cli.js watch",
    "test:ui": "node node_modules/vitest/dist/cli.js --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit:run && npm run test:e2e",
    "coverage": "vitest run --coverage",
    "coverage:report": "vitest run --coverage && open coverage/index.html",
    "prepare": "husky install",
    "validate:icons": "node scripts/validate-icons-simple.js",
    "test:app-loading": "node test-app-loading.cjs",
    "test:auth-bypass": "node test-auth-bypass.cjs",
    "test:simple": "node test-simple.cjs",
    "test:controlled-inputs": "node test-controlled-inputs.cjs",
    "fix:controlled-inputs": "node fix-controlled-inputs.cjs",
    "test:queryclient": "node test-queryclient.cjs",
    "test:data-loading": "node test-data-loading.cjs",
    "test:comprehensive": "node test-comprehensive.cjs",
    "test:final": "node test-final-verification.cjs",
    "collect-history": "node scripts/collectHistoryData.js",
    "collect-all-history": "node scripts/collectAllHistoricalData.js",
    "monitor-alerts": "node scripts/monitorVolumeAlerts.js",
    "db:migrate": "cd server && npm run db:migrate",
    "db:reset": "cd server && npm run db:reset",
    "db:create": "cd server && npm run db:create",
    "db:deploy": "npx prisma migrate deploy",
    "db:push": "cd server && npx drizzle-kit push",
    "test:suggested-items": "vitest run src/pages/SuggestedItems",
    "test:suggested-items-api": "vitest run server/src/**/*suggested*",
    "test:e2e-suggested": "playwright test suggested-items --config=tests/e2e/playwright.config.ts",
    "test:suggested-items-all": "npm run test:suggested-items && npm run test:suggested-items-api",
    "docs:sync": "bash scripts/sync-claude-docs.sh",
    "docs:claude": "bash scripts/claude-docs-helper.sh",
    "docs:claude:help": "bash scripts/claude-docs-helper.sh help",
    "docs:claude:search": "bash scripts/claude-docs-helper.sh search",
    "docs:claude:list": "bash scripts/claude-docs-helper.sh list",
    "docs:claude:map": "bash scripts/claude-docs-helper.sh map",
    "test:audit": "node scripts/audit-tests.js",
    "test:create": "node scripts/create-test-template.js",
    "backup": "node scripts/backup.js",
    "backup:create": "node scripts/backup.js create",
    "backup:restore": "node scripts/backup.js restore",
    "backup:list": "node scripts/backup.js list",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "backup:verify": "node scripts/backup.js verify",
    "backup:schedule": "node scripts/backup-schedule.js start",
    "backup:status": "node scripts/backup.js status",
    "backup:interactive": "node scripts/backup.js interactive"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "6.0.22",
    "@mantine/dates": "6.0.22",
    "@mantine/form": "6.0.22",
    "@mantine/hooks": "6.0.22",
    "@mantine/notifications": "6.0.22",
    "@mantine/prism": "6.0.22",
    "@mantine/utils": "6.0.22",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^5.0.0",
    "@ravnhq/eslint-config": "^0.0.33",
    "@tabler/icons-react": "^2.23.0",
    "@trpc/react-query": "^11.4.2",
    "@vercel/postgres": "^0.4.1",
    "apexcharts": "^4.0.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^2.2.1",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^9.23.0",
    "firebaseui": "^6.0.2",
    "hoist-non-react-statics": "^3.3.2",
    "klona": "^2.0.6",
    "lightweight-charts": "^4.0.0",
    "moment": "^2.29.4",
    "nodemon": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "recharts": "^2.15.3",
    "sass": "^1.63.4",
    "stripe": "^19.0.0",
    "stylis": "^4.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^26.1.0",
    "prisma": "^5.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
